    "expo-linking": "~7.0.3",
    "expo-secure-store": "~14.0.0",
    "expo-speech": "~13.0.0",
    "expo-splash-screen": "~0.29.18",
    "expo-status-bar": "~2.0.0",
    "expo-symbols": "~0.2.0",
    "expo-system-ui": "~4.0.6",
    "expo-updates": "~0.26.10",
    "expo-web-browser": "~14.0.1",
    "llama.rn": "^0.4.3",
    "mobx": "6.10.2",
